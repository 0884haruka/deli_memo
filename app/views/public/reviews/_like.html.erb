<% if review.liked_by?(current_member) %>
  <%= link_to review_likes_path(review), method: :delete, remote: :true do %>
    <i class="text-danger ">♥</i><%= review.likes.count %>いいね
　<% end %>
<% else %>
　<%= link_to review_likes_path(review), method: :post, remote: true do %>
    <i class="text-danger ">♡</i><%= review.likes.count %>いいね
　<% end %>
<% end %>